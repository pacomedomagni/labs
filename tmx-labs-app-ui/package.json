{
  "name": "tmx-labs-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run lint && ng build -c production --base-href=/",
    "watch": "ng build --watch --configuration development",
    "test-ci": "ng test --browsers=ChromeHeadlessCI --watch=false --code-coverage",
    "test": "ng test --browsers=ChromeDebug",
    "lint": "eslint --ext .ts ./src/app",
    "lint-fix": "eslint --ext .ts ./src/app/ --fix"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 4,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.15",
    "@angular/cdk": "^20.2.0",
    "@angular/common": "^20.3.15",
    "@angular/compiler": "^20.3.15",
    "@angular/core": "^20.3.15",
    "@angular/forms": "^20.3.15",
    "@angular/google-maps": "^20.2.14",
    "@angular/material": "^20.2.0",
    "@angular/material-luxon-adapter": "^20.2.0",
    "@angular/platform-browser": "^20.3.15",
    "@angular/router": "^20.3.15",
    "@fortawesome/angular-fontawesome": "^3.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@ngneat/until-destroy": "^10.0.0",
    "@pgr-cla/core-ui-components": "^34.8.0",
    "angular-oauth2-oidc": "^20.0.2",
    "chart.js": "^4.5.1",
    "clone": "^2.1.2",
    "ngx-cookie-service": "^20.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "xng-breadcrumb": "^14.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.13",
    "@angular/cli": "^20.3.13",
    "@angular/compiler-cli": "^20.3.15",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.2.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "puppeteer": "^24.34.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.40.0"
  },
  "overrides": {
    "qs": "6.14.1",
    "@angular/cli": {
      "@modelcontextprotocol/sdk": "1.25.2"
    },
    "@andular/compiler": "^20.3.15",
    "tar": "^7.5.7",
    "lodash": "^4.17.23"
  }
}
